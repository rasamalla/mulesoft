    <http:listener-config name="HTTP_Listener_Configuration" host="localhost" port="8081" basePath="/hello" doc:name="HTTP Listener Configuration"/>
    <flow name="hellogitjenkinsFlow">
        <http:listener config-ref="HTTP_Listener_Configuration" path="/hellogitjenkins"  doc:name="HTTP"/>
        <logger message="&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;flow has started&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;&quot;" level="INFO" doc:name="Logger"/>
        <set-variable variableName="Name" value="hello" doc:name="Variable"/>
        <flow-ref name="hellogitjenkinsSub_Flow" doc:name="Flow Reference"/>
        <set-payload value="#[flowVars.Name]" doc:name="Set Payload"/>
    </flow>
    <sub-flow name="hellogitjenkinsSub_Flow">
        <logger message="__________________SubFlow-----------------------#[flowVars.Name]" level="INFO" doc:name="Logger"/>
        <set-payload value="#[flowVars.Name]" doc:name="Set Payload"/>
    </sub-flow>
